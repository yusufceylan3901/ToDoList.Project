                            <p>Delete Profile</p>
                        </a>
                    </li>*@
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            @Html.Partial("_Navbar")

            <!-- End Navbar -->
            @RenderBody()


            @*@Html.Partial("_Footer")*@
        </div>
    </div>
